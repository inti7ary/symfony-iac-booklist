{% extends 'layout.html.twig' %}
{% block title %} Профиль {% endblock %}

{%block app %} 

<user_profile 

{% if user_id is defined %}

    :id={{user_id}}

    {% if user_id == app.user.id %}
    :enable_editing=true
    {% endif %}

{% else %}

    :id={{app.user.id|number_format}}
    :enable_editing=true

{% endif %}
>

</user_profile>



{% endblock %}